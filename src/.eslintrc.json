{
  "plugins": [
    "angular",
    "mocha",
    "promise"
  ],
  "extends": [
    "../.eslintrc.json",
    "angular"
  ],
  "env": {
    "browser": true,
    "mocha": true,
    "node": false
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "globals": {
    "inject": true,
    "require": true
  },
  "rules": {
    "angular/module-getter": "off",
    "angular/no-service-method": "off",
    "angular/on-watch": "off",
    "no-console": "error",
    "no-duplicate-imports": "error",
    "no-useless-rename": "error",
    "sort-imports": "error"
  }
}
